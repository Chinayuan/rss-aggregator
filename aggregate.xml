---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
	<channel>
		<title>{{ site.title }}</title>
		<description>{{ site.project_summary }}</description>
		<link>{{ site.url }}</link>
		<language>en-us</language>
		{% for item in site.data.feed-entries limit:50 %}

			{% assign d = item.pubDate | split: '/' %}
			{{ d[0] }} -- {{ d[1] }} -- {{ d[2] }}

				{% if item.title != null %}<item>
				<title>{{ item.title }}</title>
				<description></description>
				<pubDate>{{ item.pubDate | date_to_string }}</pubDate>
				<link>{{ item.link }}}</link>
				<guid isPermaLink="true">{{ item.link }}</guid>
			</item>{% endif %}{% endfor %}
	</channel>
</rss>
