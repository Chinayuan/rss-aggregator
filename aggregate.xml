---
layout: none
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ site.title }}</title>
		<description>{{ site.project_summary }}</description>
		<link>{{ site.url }}</link>
		<language>en-us</language>
		<atom:link href="{{ site.url }}/aggregate.xml" rel="self" type="application/rss+xml" /
		{% for item in site.data.feed-entries limit:50 %}
			{% assign datePublished = '' %}
			{% assign d = item.pubDate | split: '/' %}
			{% assign datePublished = datePublished | append: d[1] %}
			{% assign datePublished = datePublished | append: '/' %}
			{% assign datePublished = datePublished | append: d[0] %}
			{% assign datePublished = datePublished | append: '/' %}
			{% assign datePublished = datePublished | append: d[2] %}
			{{ assign itemLink = item.link}}
				{% if item.title != null %}<item>
				<title>{{ item.title }}</title>
				<description></description>
				<pubDate>{{ datePublished | date_to_rfc822 }}</pubDate>
				<link>{{ item.link }}</link>
				<guid isPermaLink="true">{{ item.link }}</guid>
			</item>{% endif %}{% endfor %}
	</channel>
</rss>
