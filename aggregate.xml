---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
	<channel>
		<title>{{ site.title }}</title>
		<description>{{ site.project_summary }}</description>
		<link>{{ site.url }}</link>
		<language>en-us</language>
		<atom:link href="{{ site.url }}/aggregate.xml" rel="self" type="application/rss+xml" />
		{% for item in site.data.feed-entries limit:50 %}
				{% if item.title != null %}<item>
				<title>{{ item.title }}</title>
				<description></description>
				<pubDate>{{ item.pubDate | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
				<link>{{ item.feed }}</link>
				<guid isPermaLink="true">{{ item.link }}</guid>
			</item>{% endif %}{% endfor %}
	</channel>
</rss>
